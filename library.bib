author = {Merion, Robert M. and Sharma, Pratima and Mathur, Amit K. and Schaubel, Douglas E.},
doi = {10.1111/j.1432-2277.2011.01274.x},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Merion et al. - 2011 - Evidence-based development of liver allocation A review.pdf:pdf},
isbn = {1432-2277 (Electronic)$\backslash$r0934-0874 (Linking)},
issn = {09340874},
journal = {Transplant International},
keywords = {donation,donor management,donor registries,education,liver,liver clinical,organ allocation,organ preservation and procurement,pediatric transplantation},
number = {10},
pages = {965--972},
pmid = {21615548},
title = {{Evidence-based development of liver allocation: A review}},
volume = {24},
year = {2011}
}
@article{Mellinger2018,
abstract = {Aims: Alcohol-related liver disease (ALD) is the second leading cause of liver transplantation performed in the USA and Europe. We aimed to provide a narrative review of the major ethical issues governing transplantation for ALD. Methods: We performed a narrative review of the ethical concepts in organ allocation for ALD, including alcoholic hepatitis. Results: Ethical concerns regarding organ allocation for ALD involve issues of urgency, utility and justice. Post-transplant outcomes for ALD patients are good and ethical considerations limiting organs solely because of alcohol etiology do not bear scrutiny. Conclusion: ALD will continue to be a major cause for liver failure. The main criteria for transplant in ALD should be the patient's risk of return to harmful drinking, alongside standard assessments of physical and psychosocial fitness for transplant.},
author = {Mellinger, Jessica L. and Volk, Michael L.},
doi = {10.1093/alcalc/agx105},
