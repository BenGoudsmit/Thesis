journal = {European Annals of Otorhinolaryngology, Head and Neck Diseases},
keywords = {a homogeneous disease in,a review of,and,articles,dejonckere,four dimensions proposed by,into account the first,new and simpler protocol,on the basis of,on unilateral vocal fold,only 33 of them,paralyses,since 2001 retrieving 452,terms of physiology,this protocol was elaborated,to elaborate a,uvfp,voice assessment,voice outcome,we chose to focus,were using methods taking,which represents},
