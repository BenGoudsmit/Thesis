doi = {10.1002/hep.25603},
file = {:C$\backslash$:/Users/ben{\_}g/Dropbox/TRX/Lever/Studies/Benefit/NSE/Literatuur/hep.25603.pdf:pdf},
issn = {02709139},
journal = {Hepatology},
