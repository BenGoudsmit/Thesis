keywords = {Dysphonia assessment,Meta-analysis,Phonosurgery,Standardisation,Voice assessment},
number = {2},
pages = {77--82},
pmid = {11307610},
title = {{A basic protocol for functional assessment of voice pathology, especially for investigating the efficacy of (phonosurgical) treatments and evaluating new assessment techniques: Guideline elaborated by the Committee on Phoniatrics of the European Laryngolo}},
