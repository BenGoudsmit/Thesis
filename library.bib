isbn = {1434-4726 (Electronic)$\backslash$n0937-4477 (Linking)},
issn = {09374477},
journal = {European Archives of Oto-Rhino-Laryngology},
keywords = {Larynx,Three-dimensional computed tomography (3D CT),Unilateral vocal fold paralysis (UVFP)},
number = {1},
