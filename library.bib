}
@article{Pajouheshnia2020,
abstract = {Failure to account for time-dependent treatment use when developing a prognostic model can result in biased future predictions. We reviewed currently available methods to account for treatment use when developing a prognostic model. First, we defined the estimands targeted by each method and examined their mechanisms of action with directed acyclic graphs (DAGs). Next, methods were implemented in data from 1,906 patients; 325 received selective $\beta$-blockers (SBBs) during follow-up. We demonstrated seven Cox regression modeling strategies: (a) ignoring SBB treatment; (b) excluding SBB users or (c) censoring them when treated; (d) inverse probability of treatment weighting after censoring (IPCW), including SBB treatment as (e) a binary or (f) a time-dependent covariate; and (g) marginal structural modeling (MSM). Using DAGs, we demonstrated IPCW and MSM have the best properties and target a similar estimand. In the case study, compared to (a), approaches (b) and (e) provided predictions that were 1{\%} and 2{\%} higher on average. Performance (c-statistic, Brier score, calibration slope) varied minimally between approaches. Our review of methods confirmed that ignoring treatment is theoretically inferior, but differences between the prediction models obtained using different methods can be modest in practice. Future simulation studies and applications are needed to assess the value of applying IPCW or MSM to adjust for treatments in different treatment and disease settings.},
author = {Pajouheshnia, Romin and Schuster, Noah A. and Groenwold, Rolf H.H. and Rutten, Frans H. and Moons, Karel G.M. and Peelen, Linda M.},
doi = {10.1111/stan.12193},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pajouheshnia et al. - 2020 - Accounting for time-dependent treatment use when developing a prognostic model from observational data A re.pdf:pdf},
issn = {14679574},
journal = {Statistica Neerlandica},
keywords = {Cox regression,inverse probability weights,marginal structural model,prediction,prognosis},
number = {1},
pages = {38--51},
title = {{Accounting for time-dependent treatment use when developing a prognostic model from observational data: A review of methods}},
volume = {74},
year = {2020}
}
