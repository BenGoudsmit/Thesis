doi = {10.1177/0194599812438171},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bergeron et al. - 2012 - Measurable Progress in Female Authorship in Otolaryngology.pdf:pdf},
