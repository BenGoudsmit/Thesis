}
@article{Sullivan2007,
abstract = {BACKGROUND: Corticosteroids and antiviral agents are widely used to treat the early stages of idiopathic facial paralysis (i.e., Bell's palsy), but their effectiveness is uncertain. METHODS: We conducted a double-blind, placebo-controlled, randomized, factorial trial involving patients with Bell's palsy who were recruited within 72 hours after the onset of symptoms. Patients were randomly assigned to receive 10 days of treatment with prednisolone, acyclovir, both agents, or placebo. The primary outcome was recovery of facial function, as rated on the House-Brackmann scale. Secondary outcomes included quality of life, appearance, and pain. RESULTS: Final outcomes were assessed for 496 of 551 patients who underwent randomization. At 3 months, the proportions of patients who had recovered facial function were 83.0{\%} in the prednisolone group as compared with 63.6{\%} among patients who did not receive prednisolone (P{\textless}0.001) and 71.2{\%} in the acyclovir group as compared with 75.7{\%} among patients who did not receive acyclovir (adjusted P=0.50). After 9 months, these proportions were 94.4{\%} for prednisolone and 81.6{\%} for no prednisolone (P{\textless}0.001) and 85.4{\%} for acyclovir and 90.8{\%} for no acyclovir (adjusted P=0.10). For patients treated with both drugs, the proportions were 79.7{\%} at 3 months (P{\textless}0.001) and 92.7{\%} at 9 months (P{\textless}0.001). There were no clinically significant differences between the treatment groups in secondary outcomes. There were no serious adverse events in any group. CONCLUSIONS: In patients with Bell's palsy, early treatment with prednisolone significantly improves the chances of complete recovery at 3 and 9 months. There is no evidence of a benefit of acyclovir given alone or an additional benefit of acyclovir in combination with prednisolone. (Current Controlled Trials number, ISRCTN71548196 [controlled-trials.com].).},
annote = {NEJM aciclovir trial: verkeerde medicatie, focus is ook op Bell (maar kan relevant zijn vanwege deels Ramsay Hunt?)},
author = {Sullivan, Frank M. and Swan, Iain R.C. and Donnan, Peter T. and Morrison, Jillian M. and Smith, Blair H. and McKinstry, Brian and Davenport, Richard J. and Vale, Luke D. and Clarkson, Janet E. and Hammersley, Victoria and Hayavi, Sima and McAteer, Anne and Stewart, Ken and Daly, Fergus},
doi = {10.1056/NEJMoa072006},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sullivan et al. - 2007 - Early Treatment with Prednisolone or Acyclovir in Bell's Palsy.pdf:pdf},
