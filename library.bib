file = {:C$\backslash$:/Users/ben{\_}g/Dropbox/TRX/Lever/Studies/NSE/Literatuur/1-s2.0-S0016508515007192-main.pdf:pdf},
issn = {15280012},
journal = {Gastroenterology},
keywords = {Exception Points,Liver Cancer,Priority,UNOS},
number = {3},
pages = {669--680},
pmid = {26021233},
publisher = {Elsevier, Inc},
title = {{Comparison of Liver Transplant-Related Survival Benefit in Patients with Versus Without Hepatocellular Carcinoma in the United States}},
url = {http://dx.doi.org/10.1053/j.gastro.2015.05.025},
volume = {149},
