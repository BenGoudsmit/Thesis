journal = {Journal of Hepatology},
keywords = {Burden,Chronic liver disease},
number = {1},
pages = {151--171},
publisher = {European Association for the Study of the Liver},
title = {{Burden of liver diseases in the world}},
url = {https://doi.org/10.1016/j.jhep.2018.09.014},
volume = {70},
year = {2019}
