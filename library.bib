issn = {10976833},
journal = {Journal of Pediatrics},
keywords = {allograft,liver,pediatric,split,transplant},
pages = {148--153.e2},
publisher = {Elsevier Inc.},
title = {{Fifteen-Year Trends in Pediatric Liver Transplants: Split, Whole Deceased, and Living Donor Grafts}},
url = {https://doi.org/10.1016/j.jpeds.2017.11.015},
volume = {196},
