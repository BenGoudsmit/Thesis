doi = {10.1161/JAHA.118.009555},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bouwens et al. - 2019 - Temporal patterns of 14 blood biomarker candidates of cardiac remodeling in relation to prognosis of patients wi.pdf:pdf},
