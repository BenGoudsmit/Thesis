issn = {16006135},
journal = {American Journal of Transplantation},
keywords = {Exception,Liver transplantation,MELD,Organ allocation,UNOS region,Waiting list},
number = {11},
pages = {2353--2361},
pmid = {22029544},
title = {{Regional variability in symptom-based MELD exceptions: A response to organ shortage?}},
volume = {11},
