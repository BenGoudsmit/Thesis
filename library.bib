issn = {0007-1323},
journal = {The British journal of surgery},
keywords = {Antibiotic Prophylaxis,Antibiotic Prophylaxis: methods,Bacterial Infections,Bacterial Infections: prevention {\&} control,Female,Hepatectomy,Hepatectomy: methods,Humans,Length of Stay,Liver Diseases,Liver Diseases: surgery,Male,Middle Aged,Postoperative Complications,Postoperative Complications: prevention {\&} control,Prospective Studies},
number = {4},
pages = {489--93},
pmid = {9607529},
title = {{Prospective randomized trial of systemic antibiotics in patients undergoing liver resection.}},
