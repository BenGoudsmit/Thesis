file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Furuta et al. - 2008 - Surgical complications of salvage total laryngectomy following concurrent chemoradiotherapy.pdf:pdf},
