journal = {American Journal of Neuroradiology},
number = {1},
pages = {402--408},
pmid = {16484419},
title = {{Effects of dexamethasone on cerebral perfusion and water diffusion in patients with high-grade glioma}},
volume = {27},
year = {2006}
}
