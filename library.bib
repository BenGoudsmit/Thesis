@article{Wiesner2003,
abstract = {Background {\&} Aims: A consensus has been reached that liver donor allocation should be based primarily on liver disease severity and that waiting time should not be a major determining factor. Our aim was to assess the capability of the Model for End-Stage Liver Disease (MELD) score to correctly rank potential liver recipients according to their severity of liver disease and mortality risk on the OPTN liver waiting list. Methods: The MELD model predicts liver disease severity based on serum creatinine, serum total bilirubin, and INR and has been shown to be useful in predicting mortality in patients with compensated and decompensated cirrhosis. In this study, we prospectively applied the MELD score to estimate 3-month mortality to 3437 adult liver transplant candidates with chronic liver disease who were added to the OPTN waiting list at 2A or 2B status between November, 1999, and December, 2001. Results: In this study cohort with chronic liver disease, 412 (12{\%}) died during the 3-month follow-up period. Waiting list mortality increased directly in proportion to the listing MELD score. Patients having a MELD score {\textless}9 experienced a 1.9{\%} mortality, whereas patients having a MELD score ≥40 had a mortality rate of 71.3{\%}. Using the c-statistic with 3-month mortality as the end point, the area under the receiver operating characteristic (ROC) curve for the MELD score was 0.83 compared with 0.76 for the Child-Turcotte-Pugh (CTP) score (P {\textless} 0.001). Conclusions: These data suggest that the MELD score is able to accurately predict 3-month mortality among patients with chronic liver disease on the liver waiting list and can be applied for allocation of donor livers.},
annote = {eerste studie die MELD voor wachtlijsters valideerde.},
author = {Wiesner, Russell and Edwards, Erick and Freeman, Richard and Harper, Ann and Kim, Ray and Kamath, Patrick and Kremers, Walter and Lake, John and Howard, Todd and Merion, Robert M. and Wolfe, Robert A. and Krom, Ruud and Colombani, Paul M. and Cottingham, Paige C. and Dunn, Stephen P. and Fung, John J. and Hanto, Douglas W. and McDiarmid, Sue V. and Rabkin, John M. and Teperman, Lewis W. and Turcotte, Jeremiah G. and Wegman, Lynn Rothberg},
doi = {10.1053/gast.2003.50016},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wiesner et al. - 2003 - Model for end-stage liver disease (MELD) and allocation of donor livers.pdf:pdf},
