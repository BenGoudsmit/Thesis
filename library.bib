@article{Ambrosch2007,
abstract = {PURPOSE OF REVIEW$\backslash$n$\backslash$nTransoral laser microsurgery has developed in recent years into a surgical method that combines a minimally invasive approach with the surgical precision of laser and microscope. The outcomes of laser microsurgical treatment of laryngeal carcinomas are reviewed and compared with the results of competitive standard procedures.$\backslash$n$\backslash$n$\backslash$nRECENT FINDINGS$\backslash$n$\backslash$nLaser microsurgery is widely acknowledged to have advantages in the treatment of early glottic carcinoma. In the treatment of glottic carcinoma causing impaired mobility or fixation of the vocal cord the role of laser surgery has not yet been definitively assessed. Based on published results, primary laser therapy can achieve local tumor control with a functional residual larynx in approximately 70-80{\%} of cases. In patients with early or moderately advanced supraglottic carcinoma, laser microsurgery is comparable to open supraglottic laryngectomy in terms of local control and survival. With regard to organ preservation, laser microsurgery is comparable to open supraglottic laryngectomy but superior to radiotherapy. Microsurgery can preserve functionally important structures, allowing for early swallowing rehabilitation while avoiding tracheotomy.$\backslash$n$\backslash$n$\backslash$nSUMMARY$\backslash$n$\backslash$nThis review elucidates the role of laser microsurgical partial resections of the larynx in comparison with other treatment modalities.},
author = {Ambrosch, Petra},
doi = {10.1097/MOO.0b013e3280147336},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ambrosch - 2007 - The role of laser microsurgery in the treatment of laryngeal cancer.pdf:pdf},
isbn = {1068-9508 (Print)$\backslash$r1068-9508 (Linking)},
issn = {1068-9508},
journal = {Current opinion in otolaryngology {\&} head and neck surgery},
keywords = {82,88,co 2 laser microsurgery,curr opin otolaryngol head,larynx carcinoma,neck surg 15,organ,preservation,treatment,{\ss} 2007 lippincott williams},
number = {2},
pages = {82--88},
pmid = {17413407},
title = {{The role of laser microsurgery in the treatment of laryngeal cancer}},
