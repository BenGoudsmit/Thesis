@article{Luo2018,
abstract = {Currently, there is debate among the liver transplant community regarding the most appropriate mechanism for organ allocation: urgency-based (MELD) versus utility-based (survival benefit). We hypothesize that MELD and survival benefit are closely associated, and therefore, our current MELD-based allocation already reflects utility-based allocation. We used generalized gamma parametric models to quantify survival benefit of LT across MELD categories among 74 196 adult liver-only active candidates between 2006 and 2016 in the United States. We calculated time ratios (TR) of relative life expectancy with transplantation versus without and calculated expected life years gained after LT. LT extended life expectancy (TR {\textgreater} 1) for patients with MELD {\textgreater} 10. The highest MELD was associated with the longest relative life expectancy (TR = 1.05 1.20 1.37 for MELD 11-15, 2.29 2.49 2.70 for MELD 16-20, 5.30 5.72 6.16 for MELD 21-25, 15.12 16.35 17.67 for MELD 26-30; 39.26 43.21 47.55 for MELD 31-34; 120.04 128.25 137.02 for MELD 35-40). As a result, candidates with the highest MELD gained the most life years after LT: 0.2, 1.5, 3.5, 5.8, 6.9, 7.2 years for MELD 11-15, 16-20, 21-25, 26-30, 31-34, 35-40, respectively. Therefore, prioritizing candidates by MELD remains a simple, effective strategy for prioritizing candidates with a higher transplant survival benefit over those with lower survival benefit.},
annote = {gamma regression?
MELD in time-varying fashion
Higher MELD: more survival benefit},
author = {Luo, Xun and Leanza, Joseph and Massie, Allan B. and Garonzik-Wang, Jacqueline M. and Haugen, Christine E. and Gentry, Sommer E. and Ottmann, Shane E. and Segev, Dorry L.},
doi = {10.1111/ajt.14660},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Luo et al. - 2018 - MELD as a metric for survival benefit of liver transplantation.pdf:pdf},
issn = {16006143},
