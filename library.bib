file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jorjani et al. - 2016 - An updated human snoRNAome.pdf:pdf},
isbn = {13624962 (Electronic)},
issn = {13624962},
journal = {Nucleic Acids Research},
number = {11},
pages = {5068--5082},
pmid = {27174936},
title = {{An updated human snoRNAome}},
volume = {44},
year = {2016}
}
@article{Remacle2009a,
