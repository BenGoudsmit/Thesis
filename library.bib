@article{Nam2013,
abstract = {BACKGROUND : Recent evidence has suggested that intra-arterial thrombolysis may provide benefit beyond intravenous thrombolysis in ischemic stroke patients. Previous meta-analyses have only compared intra-arterial thrombolysis with standard treatment without thrombolysis. The objective was to review the benefits and harms of intra-arterial thrombolysis in ischemic stroke patients. METHODS : We undertook a meta-analysis of randomized controlled trials comparing the efficacy and safety of intra-arterial thrombolysis with either standard treatment or intravenous thrombolysis following acute ischemic stroke. Primary outcomes included poor functional outcomes (modified Rankin Scale 3-6), mortality, and symptomatic intracranial hemorrhage. Study quality was assessed, and outcomes were stratified by comparison treatment received. RESULTS : Four trials (n = 351) comparing intra-arterial thrombolysis with standard treatment were identified. Intra-arterial thrombolysis reduced the risk of poor functional outcomes (modified Rankin Scale 3-6) [relative risk (RR) = 0{\textperiodcentered}80; 95{\%} confidence interval = 0{\textperiodcentered}67-0{\textperiodcentered}95; P = 0{\textperiodcentered}01]. Mortality was not increased (RR = 0{\textperiodcentered}82; 95{\%} confidence interval = 0{\textperiodcentered}56-1{\textperiodcentered}21; P = 0{\textperiodcentered}32); however, risk of symptomatic intracranial hemorrhage was nearly four times more likely (RR = 3{\textperiodcentered}90; 95{\%} confidence interval = 1{\textperiodcentered}41-10{\textperiodcentered}76; P = 0{\textperiodcentered}006). Two trials (n = 81) comparing intra-arterial thrombolysis with intravenous thrombolysis were identified. Intra-arterial thrombolysis was not found to reduce poor functional outcomes (modified Rankin Scale 3-6) (RR = 0{\textperiodcentered}68; 95{\%} confidence interval = 0{\textperiodcentered}46-1{\textperiodcentered}00; P = 0{\textperiodcentered}05). Mortality was not increased (RR = 1{\textperiodcentered}12; 95{\%} confidence interval = 0{\textperiodcentered}47-2{\textperiodcentered}68; P = 0{\textperiodcentered}79); neither was symptomatic intracranial hemorrhage (RR = 1{\textperiodcentered}13; 95{\%} confidence interval = 0{\textperiodcentered}32-3{\textperiodcentered}99; P = 0{\textperiodcentered}85). Differences in time from symptom onset-to-treatment and type of thrombolytic administered were found across the trials. CONCLUSIONS : This analysis finds a modest benefit of intra-arterial thrombolysis over standard treatment, although it does not find a clear benefit of intra-arterial thrombolysis over intravenous thrombolysis in acute ischemic stroke patients. However, few trials, small sample sizes, and indirectness limit the strength of evidence.},
author = {Nam, Julian and Jing, He and O'Reilly, Daria},
doi = {10.1111/j.1747-4949.2012.00914.x},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nam, Jing, O'Reilly - 2013 - Intra-arterial thrombolysis vs. standard treatment or intravenous thrombolysis in adults with acute ischemi.pdf:pdf},
issn = {1747-4949},
journal = {International journal of stroke : official journal of the International Stroke Society},
keywords = {intra-arterial thrombolysis,intravenous},
number = {January},
pages = {1--10},
pmid = {23294872},
title = {{Intra-arterial thrombolysis vs. standard treatment or intravenous thrombolysis in adults with acute ischemic stroke: a systematic review and meta-analysis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23294872},
volume = {10},
year = {2013}
}
@article{Rizopoulos2017a,
abstract = {"Preface Joint models for longitudinal and time-to-event data have become a valuable tool in the analysis of follow-up data. These models are applicable mainly in two settings: First, when focus is in the survival outcome and we wish to account for the effect of an endogenous time-dependent covariate measured with error, and second, when focus is in the longitudinal outcome and we wish to correct for nonrandom dropout. Due to their capability to provide valid inferences in settings where simpler statistical tools fail to do so, and their wide range of applications, the last 25 years have seen many advances in the joint modeling field. Even though interest and developments in joint models have been widespread, information about them has been equally scattered in articles, presenting recent advances in the field, and in book chapters in a few texts dedicated either to longitudinal or survival data analysis. However, no single monograph or text dedicated to this type of models seems to be available. The purpose in writing this book, therefore, is to provide an overview of the theory and application of joint models for longitudinal and survival data. In the literature two main frameworks have been proposed, namely the random effects joint model that uses latent variables to capture the associations between the two outcomes (Tsiatis and Davidian, 2004), and the marginal structural joint models based on G estimators (Robins et al., 1999, 2000). In this book we focus in the former. Both subfields of joint modeling, i.e., handling of endogenous time-varying covariates and nonrandom dropout, are equally covered and presented in real datasets"-- Preface -- 1. Introduction -- 1.1. Goals -- 1.2. Motivating Studies -- 1.2.1. Primary Biliary Cirrhosis Data -- 1.2.2. AIDS Data -- 1.2.3. Liver Cirrhosis Data -- 1.2.4. Aortic Valve Data -- 1.2.5. Other Applications -- 1.3. Inferential Objectives in Longitudinal Studies -- 1.3.1. Effect of Covariates on a Single Outcome -