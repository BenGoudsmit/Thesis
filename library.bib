file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hou et al. - 2013 - Peritumoral brain edema in intracranial meningiomas the emergence of vascular endothelial growth factor-directed the.pdf:pdf},
issn = {1092-0684},
journal = {Neurosurgical focus},
keywords = {Brain Edema,Brain Edema: etiology,Brain Edema: therapy,Databases, Bibliographic,Databases, Bibliographic: statistics {\&} numerical d,Humans,Meningeal Neoplasms,Meningeal Neoplasms: complications,Meningioma,Meningioma: complications,Vascular Endothelial Growth Factor A,Vascular Endothelial Growth Factor A: metabolism,Vascular Endothelial Growth Factor A: therapeutic},
