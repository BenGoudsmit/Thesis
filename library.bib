isbn = {0023-852X},
issn = {0023-852X; 0023-852X},
journal = {The Laryngoscope},
keywords = {Adaptation, Psychological,Adult,Aged,Biopsy, Needle,Carcinoma in Situ/economics/mortality/pathology/th,Carcinoma, Squamous Cell/economics/mortality/patho,Cohort Studies,Cost of Illness,Cost-Benefit Analysis,Disease-Free Survival,Female,Follow-Up Studies,Glottis/pathology,Humans,Laryngeal Neoplasms/economics/mortality/pathology/,Laryngectomy/methods/psychology,Male,Middle Aged,Neoplasm Staging,Quality of Life,Questionnaires,Radiotherapy/methods/psychology,Registries,Treatment Outcome},
