journal = {Journal of Speech Language and Hearing Research},
keywords = {1970,1983,also called the phonetogram,and,as sound pressure level,damste,frequency,i,in db,intensity,is a display of,is usually plotted vertically,phonation,phonetogram,range,schutte,seidner,the voice range profile,vocal intensity versus fundamental,voice,vrp},
number = {1},
pages = {21},
pmid = {1735970},
title = {{Acoustic Interpretation of the Voice Range Profile (Phonetogram)}},
