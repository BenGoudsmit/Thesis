issn = {16006143},
journal = {American Journal of Transplantation},
keywords = {Organ Procurement and Transplantation Network (OPT,United Network for Organ Sharing (UNOS),clinical research/practice,health services and outcomes research,liver disease malignant,liver transplantation/hepatology,organ allocation,organ procurement and allocation,recipient selection},
number = {8},
pages = {2210--2218},
