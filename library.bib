file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Aarts et al. - 2011 - Salvage Laryngectomy After Primary Radiotherapy.pdf:pdf},
isbn = {0194599810390},
issn = {0194-5998},
journal = {Otolaryngology-Head and Neck Surgery},
keywords = {laryngectomy,pharyngocutaneous fistula,salvage surgery},
number = {1},
pages = {5--9},
