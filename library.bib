issn = {16006135},
journal = {American Journal of Transplantation},
keywords = {Hepatocellular carcinoma,Liver transplantation,UCSF criteria},
number = {11},
pages = {2587--2596},
pmid = {17868066},
title = {{Liver transplantation for hepatocellular carcinoma: Validation of the UCSF-expanded criteria based on preoperative imaging}},
volume = {7},
