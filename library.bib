doi = {10.1002/hed.20279},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bernier et al. - 2005 - Defining risk levels in locally advanced head and neck cancers A comparative analysis of concurrent postoperativ.pdf:pdf},
isbn = {1043-3074 (Print)$\backslash$r1043-3074 (Linking)},
