@article{Lampon2012,
abstract = {BACKGROUND: Pro-inflammatory cytokine production is directly inhibited by acetylcholine (ACh), and a relationship between total circulating ACh hydrolytic capacity and inflammatory reactions has been previously reported. Butyrylcholinesterase (BChE) is the major ACh hydrolyzing enzyme in plasma, and the aim of our study was to evaluate its association with low-grade systemic inflammation.$\backslash$n$\backslash$nMATERIAL AND METHODS: A total of 4,077 patients clinically managed in the Cardiology, Hypertension, and Digestive Medicine Units were included in our study. Three subclinical chronic inflammatory degrees were established in accordance with the high-sensitivity C-reactive protein (hsCRP) concentrations proposed, for low ({\textless} 1 mg/L), average (1-3 mg/L), and high ({\textgreater} 3-10 mg/L) cardiovascular disease risk estimation.$\backslash$n$\backslash$nRESULTS: In male patients with subclinical chronic inflammation and hsCRP concentrations {\textless} 1 mg/L, a significant positive correlation was observed between BChE and hsCRP (p {\textless} 0.02); however, for hsCRP concentrations {\textgreater} 3 mg/L, the correlation between these variables in both sexes becomes significantly negative (p {\textless} 0.001), as in patients with acute inflammation (hsCRP {\textgreater} 10 mg/L). In all cases significant positive correlations were obtained between the BChE activities and albumin concentrations (p {\textless} 0.001).$\backslash$n$\backslash$nCONCLUSIONS: The liver production of BChE and albumin occurs in a coupled fashion, and these biochemical variables may be considered as negative inflammatory reactants, whose serum levels are inversely associated with the increasing degree of subclinical inflammation.},
author = {Lampon, Natalia and Hermida-Cadahia, Esperanza F. and Riveiro, Alberto and Tutor, J. Carlos},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lampon et al. - 2012 - Association between butyrylcholinesterase activity and low-grade systemic inflammation.pdf:pdf},
isbn = {1665-2681},
issn = {16652681},
journal = {Annals of Hepatology},
keywords = {Albumin,Butyrylcholinesterase,High-sensitivity C-reactive protein,Low-grade systemic inflammation,Procalcitonin},
number = {3},
pages = {356--363},
pmid = {22481455},
title = {{Association between butyrylcholinesterase activity and low-grade systemic inflammation}},
volume = {11},
year = {2012}
}
