isbn = {0023-852X (Print)$\backslash$r0023-852X (Linking)},
issn = {09374477},
journal = {Laryngoscope},
keywords = {External vocal fold medialization,Glottal closure insufficiency,Titanium implant,Unilateral vocal fold paralysis},
number = {4},
