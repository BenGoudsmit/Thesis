file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Keilmann et al. - 2011 - Long-term functional outcome after unilateral cordectomy.pdf:pdf},
isbn = {1423-0275 (Electronic)$\backslash$r0301-1569 (Linking)},
