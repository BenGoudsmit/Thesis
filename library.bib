issn = {1751-2433},
journal = {Expert Review of Clinical Pharmacology},
keywords = {anti-angiogenesis,brain cancer,cancer stem cells,corticosteroids,dexamethasone,giloma,neural,neurotoxicity,patients,stem cells,traditional concepts of steroid,use in brain cancer},
number = {2},
pages = {233--242},
