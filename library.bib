file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee et al. - 2016 - Voice outcome according to surgical extent of transoral laser microsurgery for T1 glottic carcinoma.pdf:pdf},
