journal = {J Laryngol Otol},
keywords = {Ambulatory Surgical Procedures/economics,Anticoagulants/*adverse effects,Feasibility Studies,Female,Humans,Laryngeal Nerves/*physiology,Male,Middle Aged,Oral Surgical Procedures/*standards,Postoperative Complications/etiology,Retrospective Studies,Treatment Outcome,Vocal Cord Paralysis/physiopathology/*surgery,Voice Quality/physiology},
