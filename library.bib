file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ang et al. - 2001 - Randomized Trial Addressing Risk Features and Time Head-and-Neck Cancer.pdf:pdf},
