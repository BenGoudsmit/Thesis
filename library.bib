journal = {Oral Oncology},
keywords = {Chemoradiotherapy,Head and neck neoplasms,Quality of life,Questionnaires,Radiotherapy,Squamous cell carcinoma,Systematic reviews},
number = {4},
pages = {254--262},
pmid = {24559650},
publisher = {Elsevier Ltd},
title = {{Health related quality of life in head and neck cancer treated with radiation therapy with or without chemotherapy: A systematic review}},
