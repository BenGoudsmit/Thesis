file = {:C$\backslash$:/Users/ben{\_}g/Dropbox/TRX/Lever/Studies/Benefit/HCC in UNOS/Literature/1-s2.0-S0168827811007720-main.pdf:pdf},
issn = {01688278},
journal = {Journal of Hepatology},
keywords = {Dropout,Intention to treat,Liver allocation,Liver tumor,Survival,Transplantation,Waiting time},
number = {3},
pages = {618--625},
