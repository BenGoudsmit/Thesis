isbn = {1433-7339 (Electronic)$\backslash$n0941-4355 (Linking)},
issn = {09414355},
journal = {Supportive Care in Cancer},
keywords = {Brain metastases,End-of-life care,Multidisciplinary team,Palliative oncology,Radiotherapy},
pages = {3379--3386},
pmid = {23934224},
title = {{Managing brain metastases patientswith and without radiotherapy: Initial lessons from a team-based consult service through a multidisciplinary integrated palliative oncology clinic}},
