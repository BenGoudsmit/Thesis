issn = {00070920},
journal = {British Journal of Cancer},
keywords = {Kaplan-Meier,Statistical methods,Survival analysis},
number = {2},
pages = {232--238},
pmid = {12865907},
title = {{Survival Analysis Part I: Basic concepts and first analyses}},
volume = {89},
