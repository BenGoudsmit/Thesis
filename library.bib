journal = {Ann Otol Rhinol Laryngol},
keywords = {- arytenoid dislocation,atrophy,cancer reconstruction,dysphonia,glottal incompetence,gore-tex,hoarseness,medialization laryngoplasty,paralysis,paresis,sulcus vocalis,thyroplasty,trauma defect},
pages = {180--184},
title = {{Medialization laryngoplasty with Gore-tex for voice restoration secondary to glottal incompetence: indications and observations.}},
