isbn = {0022-2151},
issn = {1748-5460},
journal = {The Journal of laryngology and otology},
keywords = {Fistula,Laryngectomy,Surgical Flaps},
number = {August},
pages = {1--5},
pmid = {27499210},
title = {{Role of the pectoralis major myofascial flap in preventing pharyngocutaneous fistula following salvage laryngectomy.}},
