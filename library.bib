doi = {10.1016/j.jhep.2021.07.018},
file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/D'Amico et al. - 2021 - Performance of the model for end-stage liver disease score for mortality prediction and the potential role of et.pdf:pdf},
issn = {01688278},
journal = {Journal of Hepatology},
