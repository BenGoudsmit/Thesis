keywords = {case study,dilatation,dysphagia,feeding,feeding apparatus,fistula,free tissue graft,head and neck surgery,human,laryngectomy,non profit organization,otorhinolaryngology,patient,smoking,statistical analysis,surgery,wound closure},
language = {English},
pages = {P170},
pmid = {71253766},
title = {{Outcomes in salvage laryngectomy with free tissue transfer}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}CSC=Y{\&}NEWS=N{\&}PAGE=fulltext{\&}D=emed14{\&}AN=71253766 http://digitaal.uba.uva.nl:9003/uva-linker?sid=OVID:embase{\&}id=pmid:{\&}id=doi:10.1177{\%}2F0194599812451426a142{\&}issn=0194-5998{\&}isbn={\&}volume=147{\&}issue=2{\&}spage=P170{\&}pages=P170},
