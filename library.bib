isbn = {1878-5921 (Electronic)$\backslash$n0895-4356 (Linking)},
issn = {08954356},
journal = {Journal of Clinical Epidemiology},
keywords = {GRADE,blinding,concealment,confidence in estimates,quality of evidence,risk of bias},
