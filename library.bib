file = {:C$\backslash$:/Users/ben{\_}g/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Washburn et al. - 2010 - Hepatocellular Carcinoma Patients Are Advantaged in the Current Liver Transplant Allocation System.pdf:pdf},
issn = {16006143},
journal = {American Journal of Transplantation},
keywords = {Adherence,adolescence,education,long-term outcomes,pediatric liver transplantation,psychological functioning,psychosocial issues in transplantation,young adulthood},
number = {7},
pages = {1652--1657},
pmid = {20346060},
title = {{Hepatocellular Carcinoma Patients Are Advantaged in the Current Liver Transplant Allocation System}},
volume = {10},
year = {2010}
}
